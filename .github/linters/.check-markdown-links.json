{
    "ignorePatterns": [
        // Microsoft internal links used in release issue checklists
        {
            "pattern": "^https://dotnetbinaries.blob.core.windows.net/dockerassets"
        },
        {
            "pattern": "^https://github.com/dotnet/release/blob/main/.github/ISSUE_TEMPLATE/dotnet-fx-docker-servicing-release.md"
        },
        // No support to exclude files (e.g. readme templates): https://github.com/gaurav-nelson/github-action-markdown-link-check/issues/103
        {
            "pattern": "^https://mcr.microsoft.com/v2/\\{\\{REPO\\}\\}/tags/list"
        },
        {
            "pattern": "^https://github.com/microsoft/dotnet-framework-docker/blob/main/README.\\{\\{SHORT_REPO\\}\\}.md#full-tag-listing"
        },
    ]
}
